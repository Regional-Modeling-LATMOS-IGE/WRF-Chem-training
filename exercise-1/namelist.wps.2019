&share
 wrf_core = 'ARW',
 max_dom = 1,
 start_date = '2019-10-01_00:00:00', 'YYYY-MM-DD_HH:00:00', 'YYYY-MM-DD_HH:00:00',
 end_date   = '2019-10-31_00:00:00', 'YYYY-MM-DD_HH:00:00', 'YYYY-MM-DD_HH:00:00',
 interval_seconds = 21600
 io_form_geogrid = 2,
/

&geogrid
 parent_id         =   1,
 parent_grid_ratio =   1,
 i_parent_start    =   1,
 j_parent_start    =   1,
 e_we              =   50,
 e_sn              =   45,
 geog_data_res     = 'default',
 dx = 100000,
 dy = 100000,
 map_proj = 'lambert',
 ref_lat   = 48,
 ref_lon   = 4,
 truelat1  = 45,
 truelat2  = 55,
 stand_lon = 0,
 ! This needs to be e_we/2 and e_sn/2
 ! ref_x = 86.5,
 ! ref_y = 90.0,
 geog_data_path = '<path/to/geog_wrf4>'
/

&ungrib
 out_format = 'WPS',
 prefix = '_FILE_ungrib_',
/

&metgrid
 fg_name = '_FILE_metgrid_',
 io_form_metgrid = 2,
 constants_name = 'TAVGSFC',
/
